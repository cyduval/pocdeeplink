<?xml version='1.0' encoding='utf-8'?>



<widget xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0" id="com.phonegap.vddeeplinkpoc" version="1.0.0">
    <name>TestAndroidApp</name>
    <description>TestAndroidApp app</description>
    <author href="http://www.bleau.fr" email="cyrille.duval@gmail.com">cyrille duval</author>
    <content src="index.html"/>

    <plugin name="cordova-plugin-whitelist" version="1" />


    <universal-links>
    <host name="devcyrille.videodesk.com" scheme="https">
     <path url="/deeplink.html" event="openNewsListPage" />
     <path url="/deeplinks.html" event="openNewsDetailedPage" />
    </host>
    </universal-links>


    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
